#mobile/brands
    get=>list all brands
    post=>create brand
    status: completed
#mobile/brands/del/id
    get=>fetch curresponding brand
    post=>delete brand
    status:completed
#mobile/brands/upd/id
    get=>fetch curresponding brand
    post=>update brand
    status:completed

